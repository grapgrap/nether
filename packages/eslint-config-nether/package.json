{
  "name": "eslint-config-nether",
  "type": "commonjs",
  "main": "./configs/base.cjs",
  "exports": {
    ".": {
      "require": "./configs/base.cjs"
    },
    "./react": {
      "require": "./configs/react.cjs"
    },
    "./patch/modern-module-resolution": {
      "require": "./patch/modern-module-resolution.cjs"
    },
    "./package.json": "./package.json"
